
def fetchItemsToDisplay(items, sortParameter, sortOrder, X):
    # for item in items:
    if sortParameter == 0:  # sort by name
        if sortOrder == 0:  # ascitems[0][0]
            # return (sorted(items,key=lambda x: x[0]))
            items.sort(key=lambda x: x[0])
            # return (items)
        if sortOrder == 1:  # desc
            items.sort(key=lambda x: x[0], reverse=True)
            # return (items)

    if sortParameter == 1:  # sort by relevence
        if sortOrder == 0:  # asc
            items.sort(key=lambda x: x[1])
            # return (items)
        if sortOrder == 1:  # desc
            items.sort(key=lambda x: x[1], reverse=True)
            # return (items)

    if sortParameter == 2:  # sort by  price
        if sortOrder == 0:  # asc
            items.sort(key=lambda x: x[2])
            # return (items)
        if sortOrder == 1:  # desc
            items.sort(key=lambda x: x[2], reverse=True)
            # return items

    for i in range(X):
        print (items[i][0])
